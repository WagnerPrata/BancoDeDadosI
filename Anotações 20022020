SELECTER * FROM -  MOSTRA TODAS AS LINHA DA TABELA - SEM ESPEFICICAÇÃO
SELECTER * FRAM WHERE - SELECIONA TODAS AS LINHA DA TABALA QUE POSSUI VALOR ESPECIFICADO
SELECTER *FROM WHERE LIKE - MOSTRA CADEIA DE CARACTERE QUE POSSUI NA TABELA  ['%Arroz%']
SELECTER *FROM WHERE LIKE  ESCAPE - MOSTRA CADEIA DE CARACTERE QUE POSSUI NA TABELA, PORÉM É USADO QUANDO BUSCAMOS UMA VALOR COM [%]
SELECTER DISTINTECT -  REMOVER A LINHAS REPITIDAS DE UMA TABELA 


